<AnchorPane id="AnchorPane" prefHeight="522.0" prefWidth="652.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chatbot.FXMLDocumentController">
   <children>
      <ImageView fitHeight="522.0" fitWidth="590.0" opacity="0.3" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@vectorchat.jpg" />
         </image>
      </ImageView>
      <Label layoutX="159.0" layoutY="14.0" prefHeight="43.0" prefWidth="327.0" text="SERVER SIGN-UP MENU" textFill="#ab2929" underline="true">
         <font>
            <Font name="System Bold Italic" size="25.0" />
         </font>
      </Label>
      <Label layoutX="66.0" layoutY="93.0" text="First Name :" textFill="#941d1d">
         <font>
            <Font name="System Bold Italic" size="17.0" />
         </font>
      </Label>
      <Label layoutX="67.0" layoutY="133.0" text="Last Name :" textFill="#972323">
         <font>
            <Font name="System Bold Italic" size="17.0" />
         </font>
      </Label>
      <Label layoutX="87.0" layoutY="187.0" text="Email :" textFill="#a42020">
         <font>
            <Font name="System Bold Italic" size="17.0" />
         </font>
      </Label>
      <Label layoutX="68.0" layoutY="237.0" text="UserName :" textFill="#9a2727">
         <font>
            <Font name="System Bold Italic" size="17.0" />
