      <Label layoutX="71.0" layoutY="286.0" text="Password :" textFill="#bc3636">
         <font>
            <Font name="System Bold Italic" size="17.0" />
         </font>
      </Label>
      <Button fx:id="btncancelsign" layoutX="331.0" layoutY="333.0" mnemonicParsing="false" onAction="#quitpt2" onMouseClicked="#quitpt2" text="Cancel" textFill="#b51616">
         <font>
            <Font name="System Bold Italic" size="17.0" />
         </font>
      </Button>
      <Button fx:id="btnreg" layoutX="435.0" layoutY="333.0" mnemonicParsing="false" onAction="#starter" onMouseClicked="#starter" text="Register" textFill="#c63535">
         <font>
            <Font name="System Bold Italic" size="17.0" />
         </font>
      </Button>
      <Label fx:id="labellogin" layoutX="274.0" layoutY="385.0" onMouseClicked="#aheadtologin" prefHeight="43.0" prefWidth="278.0" text="Already Have an Account ? log-In" textFill="#971616" underline="true">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </Label>
      <TextField fx:id="firstname" layoutX="211.0" layoutY="88.0" prefHeight="31.0" prefWidth="269.0" promptText="First Name here" />
      <TextField fx:id="lastname" layoutX="211.0" layoutY="131.0" prefHeight="31.0" prefWidth="269.0" promptText="Your LastName here" />
      <TextField fx:id="signemail" layoutX="211.0" layoutY="185.0" prefHeight="31.0" prefWidth="269.0" promptText="Email here" />
      <TextField fx:id="username" layoutX="211.0" layoutY="234.0" prefHeight="31.0" prefWidth="269.0" promptText="Usetname Here" />
      <PasswordField fx:id="passwordsign" layoutX="211.0" layoutY="283.0" prefHeight="31.0" prefWidth="269.0" promptText="Password here" />
