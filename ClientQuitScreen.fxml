<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chatbotcodeforclient.FXMLDocumentController">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#ffcc1f" height="371.0" layoutX="14.0" layoutY="14.0" opacity="0.38" stroke="BLACK" strokeType="INSIDE" width="576.0" />
      <ImageView fitHeight="345.0" fitWidth="549.0" layoutX="30.0" layoutY="31.0" opacity="0.24" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@vectorchat.jpg" />
         </image>
      </ImageView>
      <Label layoutX="109.0" layoutY="72.0" prefHeight="85.0" prefWidth="399.0" text="Do you really want to quit ?" textFill="#bc1919" underline="true">
         <font>
            <Font name="System Bold Italic" size="30.0" />
         </font>
      </Label>
      <Button fx:id="btnno" layoutX="326.0" layoutY="175.0" mnemonicParsing="false" onAction="#backtoLog" onMouseClicked="#backtoLog" prefHeight="69.0" prefWidth="120.0" text="NO" textFill="#d04040">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Button>
      <Button fx:id="btnaccept" layoutX="160.0" layoutY="175.0" mnemonicParsing="false" onAction="#quitandleave" onMouseClicked="#quitandleave" prefHeight="69.0" prefWidth="104.0" text="YES" textFill="#c63434">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Button>
